        "fs-extra": "^9.1.0",
        "npm-run-all": "^4.1.5",
        "sass": "^1.34.0",
        "sass-loader": "^11.1.1",
        "typescript": "^4.2.4",
        "typescript-to-lua": "^0.39.3",
        "webpack": "^5.0.0-rc.6",
